-- Management for Very high data rate Digital Subscriber Line (VDSL).

-- Define VDSL Port Config table
-- Module Identity : vdslMIBObjects/vdslConfigTable
--

    vdslConfigTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF VDSLConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION "Config Paramter for each VDSL port."
        ::= { vdslMgt 1 }

    vdslConfigEntry OBJECT-TYPE
        SYNTAX     VDSLConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION "The entry of config table."
        INDEX       { vdslPortIndex }
        ::= { vdslConfigTable 1 }

    VDSLConfigEntry ::=
        SEQUENCE {
            vdslPortIndex                       InterfaceIndex,
            vdslPortActive                      INTEGER,
            vdslPortRDL                         INTEGER,
            vdslPortProfile                     INTEGER
        }

    vdslPortIndex OBJECT-TYPE
        SYNTAX      InterfaceIndex
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION "An index value that uniquely identifies an
                   interface to a VDSL port. The
                   interface identified by a particular value of
                   this index is the same interface as identified
                   by the same value of ifIndex."
        ::= { vdslConfigEntry 1 }

    vdslPortActive OBJECT-TYPE
        SYNTAX      INTEGER
                    {
                        active(1),
                        deactive(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION "The active status of the VDSL port.
                     Set 1 to activate the port. Set 2 to
                     deactivate the port."
        ::= { vdslConfigEntry 2 }

    vdslPortRDL OBJECT-TYPE
        SYNTAX      INTEGER
                      {

                        enable(1),
                        disable(2)
                       }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION "The status of RDL,
                     set 1 to turn on RDL, set 2 to
                     turn off RDL."
        ::= { vdslConfigEntry 3 }

    vdslPortProfile OBJECT-TYPE
        SYNTAX      INTEGER
                      {

                        default(0),
                        efm-5(1),
                        efm-10(2),
                        efm-15(3),
                        public-ansi(4),
                        public-etsi(5),
                        efm-5LL(6),
                        efm-10LL(7),
                        efm-15LL(8),
                        public-ansiLL(9),
                        public-etsiLL(10),
                        efm-15-3LL(11),
                        efm-15-2LL(12),
                        efm-15-1LL(13),
                        efm-10-2LL(14),
                        user-1(15),
                        user-2(16)
                     }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION "The Profile ID of the port, starts from 0.
                     Change this value to change to different VDSL
                     line rate speeds, the number is defined
                     as: default=0, efm-5=1, efm-10=2, efm-15=3,
                     public-ansi=4, public-etsi=5, efm-5LL =6,
                     efm-10LL=7, efm-15LL=8, public-ansiLL=9,
                     public-etsiLL=10, efm-15-3LL=11, efm-152LL=12,
                     efm-15-1LL=13, efm-10-2LL=14, user-1=15, user-2=16.
                     user-1 and user-2 are user defined profiles, we
                     can config these in the vdslPortUserProfileTable."
        ::= { vdslConfigEntry 4 }
